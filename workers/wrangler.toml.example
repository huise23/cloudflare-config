name = "config-center"
main = "worker.js"
compatibility_date = "2024-01-01"

# KV 命名空间绑定
# ⚠️ 安全提示：这是示例文件，不要直接使用真实的 KV ID！
# 部署步骤：
# 1. 复制此文件为 wrangler.toml
# 2. 在 Cloudflare Dashboard 创建 KV 命名空间
# 3. 获取 KV Namespace ID
# 4. 替换下面的 your_actual_kv_namespace_id

[[kv_namespaces]]
binding = "CONFIG_KV"
id = "your_actual_kv_namespace_id"

# 环境变量
[vars]
ENVIRONMENT = "production"

# 路由配置（可选）
# route = "https://config-center.your-domain.com/*"

# Workers Dev 配置（可选）
[env.development]
kv_namespaces = [
  { binding = "CONFIG_KV", id = "dev_kv_namespace_id" }
]

# 生产环境配置（可选）
[env.production]
kv_namespaces = [
  { binding = "CONFIG_KV", id = "prod_kv_namespace_id" }
]
